{
  "name": "vue-demi-longmo",
  "version": "1.0.1",
  "description": "提供跨版本 Vue Api 支持",
  "engines": {
    "node": ">=12"
  },
  "main": "lib/index.cjs",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "bin": {
    "vue-demi-fix": "bin/vue-demi-fix.js",
